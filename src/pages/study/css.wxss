.test-redux .example-enter {
  opacity: 0.01;
  transform: translateX(100px);
}
.test-redux .example-enter.example-enter-active {
  opacity: 1;
  transform: none;
  transition: all 500ms ease-in;
}
.test-redux .example-leave {
  opacity: 1;
}
.test-redux .example-leave.example-leave-active {
  opacity: 0.01;
  transition: opacity 300ms ease-in;
}
.bubble-sort .animate {
  display: block;
  width: 500px;
  display: flex;
  align-items: flex-end;
}
.bubble-sort .item {
  transition: all .5s;
}
.bubble-sort .bubble-enter {
  opacity: 0.01;
  transform: translateX(100px);
}
.bubble-sort .bubble-enter.bubble-enter-active {
  opacity: 1;
  transform: none;
  transition: all 500ms ease-in;
}
.bubble-sort .bubble-leave {
  opacity: 1;
}
.bubble-sort .bubble-leave.bubble-leave-active {
  opacity: 0.01;
  transition: opacity 300ms ease-in;
}
